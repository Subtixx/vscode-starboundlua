{
    "$schema": "vscode://schemas/color-theme",
    "name": "Dark Starbound Edit (Visual Studio)",
    "include": "./dark_vs.json",
    "tokenColors":[
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#4887bb"
			}
		},
		{
            "name": "Keyword",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#4887bb"
			}
		},
        {
            "name": "Function Name",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "foreground": "#4EC9B0"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function.any-method",
            "settings": {
                "foreground": "#DCDCAA"
            }
        },
        {
            "name": "Library function",
            "scope": "variable.other",
            "settings": {
                "foreground": "#9cdcfe"
            }
        },
        {
            "name": "Module",
            "scope": "constant.other.module",
            "settings": {
                "foreground": "#4EC9B0"
            }
        }
    ]
}